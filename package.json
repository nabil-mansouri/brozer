{
  "name": "brozer",
  "version": "1.0.0",
  "description": "An Electron application with React and TypeScript",
  "main": "./out/main/index.js",
  "author": "example.com",
  "homepage": "https://www.electronjs.org",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "npm run build && electron-builder --win --config",
    "build:mac": "electron-vite build && electron-builder --mac --config",
    "build:linux": "electron-vite build && electron-builder --linux --config",
    "proto": "protoc --plugin=node_modules/ts-proto/protoc-gen-ts_proto --ts_proto_opt=oneof=unions --ts_proto_opt=unrecognizedEnum=false --ts_proto_opt=forceLong=long --ts_proto_opt=esModuleInterop=true --ts_proto_opt=stringEnums=true --ts_proto_opt=enumsAsLiterals=true --ts_proto_out=./src/generated proto/browser.proto"
  },
  "dependencies": {
    "@bonfida/spl-name-service": "^0.1.67",
    "@electron-toolkit/preload": "^1.0.3",
    "@electron-toolkit/utils": "^1.0.2",
    "@ensdomains/ensjs": "^3.0.0-alpha.52",
    "@unstoppabledomains/resolution": "^8.5.0",
    "classnames": "^2.3.2",
    "crypto-browserify": "^3.12.0",
    "debounce": "^1.2.1",
    "electron-dl": "^3.5.0",
    "electron-is-dev": "^2.0.0",
    "electron-localshortcut": "^3.2.1",
    "electron-updater": "^5.3.0",
    "ethers": "^5.7.2",
    "get-random-values": "^2.1.0",
    "mousetrap": "^1.6.5",
    "x509.js": "^1.0.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron/notarize": "^1.2.3",
    "@types/mousetrap": "^1.6.11",
    "@types/node": "16.18.14",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "@vitejs/plugin-react": "^3.1.0",
    "electron": "^24.0.0-beta.7",
    "electron-builder": "^23.6.0",
    "electron-vite": "^1.0.20",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "prettier": "^2.8.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-proto": "^1.145.0",
    "ts-protoc-gen": "^0.15.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.4"
  }
}
